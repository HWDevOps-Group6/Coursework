{
  "name": "healthcare-management-system",
  "version": "1.0.0",
  "description": "Healthcare Management System with NodeJS REST API, MongoDB, and JWT authentication",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "dev:auth": "cd services/auth-service && PORT=3001 npm run dev",
    "dev:all": "concurrently \"npm run dev:auth\" \"npm run dev\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "healthcare",
    "api",
    "rest",
    "mongodb",
    "jwt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
